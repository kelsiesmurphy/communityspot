{
  "name": "communityspot-v2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "pages:preview": "pnpm run build && wrangler pages dev --compatibility-date=2024-02-08 .svelte-kit/cloudflare",
    "pages:deploy": "pnpm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@playwright/test": "^1.41.2",
    "@skeletonlabs/skeleton": "^2.8.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-cloudflare": "^4.1.0",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/eslint": "8.56.0",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.0",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.4",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-tailwind-purgecss": "^0.2.0",
    "vitest": "^1.2.2",
    "wrangler": "^3.28.2"
  },
  "type": "module",
  "dependencies": {
    "@stripe/stripe-js": "^3.0.0",
    "@supabase/auth-helpers-sveltekit": "^0.12.0",
    "@supabase/supabase-js": "^2.39.4",
    "highlight.js": "^11.9.0",
    "stripe": "^14.16.0"
  }
}